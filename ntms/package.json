{
  "name": "ntms",
  "version": "0.0.35",
  "description": "A dead simple way to add and collaborate on your site's translations with the Notion API and Deepl",
  "author": "arnaudDerbey",
  "repository": "https://github.com/peers-press/ntms",
  "license": "MIT",
  "keywords": [
    "Notion",
    "CMS",
    "Notion API",
    "SSG",
    "i18n",
    "nextjs",
    "deepl",
    "translations",
    "internationalization"
  ],
  "main": "./dist/tsc/main.js",
  "types": "./dist/tsc/main.d.ts",
  "browser": "./dist/esbuild/browser.js",
  "scripts": {
    "cli": "ts-node src/cli.ts",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "build": "microbundle-crl --format modern,cjs",
    "buildc": "tsc -p tsconfig.json"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^15.0.1",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "esbuild": "^0.11.11",
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "microbundle-crl": "^0.13.11",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "node-html-parser": "^3.3.4"
